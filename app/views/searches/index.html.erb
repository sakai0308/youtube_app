<h1>Searches#index</h1>
<p>Find me in app/views/searches/index.html.erb</p>
 <div class="sky-form" role="form">
    <%= form_for :search_data, url: searches_search_path, method: :get do |f| %>
        <fieldset>
          <section>
            <label class="label">
              <%= f.label :keyword, 'キーワード'  %>
            </label>
            <label class="input state-success">
              <%= f.text_field :keyword %>
            </label>
          </section>
          <%= f.submit '検索' %>
        </fieldset>
    <% end %>
</div>
